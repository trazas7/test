# enable sideloading (you already did this once)
New-ItemProperty -Path HKCU:\Software\Microsoft\Office\16.0\WEF -Name DeveloperSideloadingAllowed -PropertyType DWord -Value 1 -Force

# sideload to desktop Outlook (no browser login involved)
npx office-addin-dev-settings sideload --app outlook --manifest .\manifest.xml
